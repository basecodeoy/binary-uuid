{
    "name": "preemstudio/laravel-binary-uuid",
    "description": "A package to easily make use of binary UUIDs with Laravel Eloquent.",
    "keywords": [
        "PreemStudio",
        "laravel",
        "laravel-binary-uuid",
        "binary",
        "uuid",
        "eloquent",
        "models",
        "mysql",
        "postgresql",
        "sqlite",
        "database"
    ],
    "homepage": "https://github.com/preemstudio/laravel-binary-uuid",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@preem.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "ramsey/uuid": "4.x-dev",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.21",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "PreemStudio\\BinaryUuid\\": "src",
            "PreemStudio\\BinaryUuid\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PreemStudio\\BinaryUuid\\BinaryUuidServiceProvider"
            ],
            "aliases": {
                "BinaryUuid": "PreemStudio\\BinaryUuid\\Facades\\BinaryUuid"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
